# Supabase Configuration (REQUIRED)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Google Calendar Integration (Optional - for calendar sync)
# If configured, Google Calendar will sync automatically without user input
VITE_GOOGLE_CALENDAR_API_KEY=your_google_calendar_api_key_here
VITE_GOOGLE_CALENDAR_CLIENT_ID=your_google_calendar_client_id_here

# Gemini API (Optional - for extractor)
# Note: client-side code can only read variables prefixed with VITE_
VITE_GEMINI_API_KEY=your_gemini_api_key_here

# OpenRouter API (Optional - alternative provider for extractor)
VITE_OPENROUTER_API_KEY=your_openrouter_api_key_here
# Optional default model id (if not set in user profile)
# e.g. google/gemini-2.0-flash-001
VITE_OPENROUTER_MODEL=
